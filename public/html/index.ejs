                <script>
                    var swiperActivity = new Swiper(".swiperActivity", {
                        slidesPerView: "auto",
                        loop: true,
                        centeredSlides: true,
                        loopAdditionalSlides: 1,
                        spaceBetween: 20,
                        // freeMode: true,
                        pagination: {
                            el: ".swiperActivity .swiper-pagination",
                            clickable: true,
                        },
                        breakpoints: {
                            640: {
                                spaceBetween: 40,
                            },
                            480: {
                                spaceBetween: 30,
                            },
                        },
                        navigation: {
                            nextEl: ".swiperActivity .swiper-button-next",
                            prevEl: ".swiperActivity .swiper-button-prev",
                        },
                    });
                    var swiperBanner = new Swiper(".swiperBanner", {
